{
  "1": {
    "name": "Heeft geldige stadspas",
    "rules": [
      {
        "type": "rule",
        "rule": "$.focus.*[@.soortProduct is 'Minimafonds' and @.typeBesluit is 'Toekenning']"
      }
    ]
  },
  "2": {
    "name": "Is 18",
    "rules": [
      {
        "type": "rule",
        "rule": "dateTime($.brp.persoon.geboortedatum) + timeDelta(18, 0, 0, 0, 0, 0) <= now()"
      }
    ]
  },
  "3": {
    "name": "Woont in gemeente Amsterdam",
    "rules": [
      {
        "type": "rule",
        "rule": "$.brp.persoon.mokum is true"
      }
    ]
  },
  "4": {
    "name": "Heeft kinderen",
    "rules": [
      {
        "type": "rule",
        "rule": "len($.brp.kinderen) >= 1"
      }
    ]
  },
  "5": {
    "name": "Is ingeschreven in Amsterdam",
    "rules": [
      {
        "type": "rule",
        "rule" : "$.brp.persoon.gemeentenaamInschrijving is Amsterdam"
      }
    ]
  },
  "6": {
    "name": "Kind is tussen 2 en 18 jaar",
    "rules": [
      {
        "type": "rule",
        "rule": "len($.brp.kinderen[now() - timeDelta(2, 0, 0, 0, 0, 0) >= dateTime(@.geboortedatum) and now() - timeDelta(18, 0, 0, 0, 0, 0) <= dateTime(@.geboortedatum)]) >= 1"
      }
    ]
  },
  "7": {
    "name": "Kind is op 30 september 2020 geen 18",
    "info": "2020-09-30T00:00:00Z",
    "rules": [
      {
        "type": "rule",
        "rule": "len($.brp.kinderen[dateTime(@.geboortedatum) => dateTime('2002-09-30T00:00:00Z')]) >= 1"
      }
    ]
  }
}